{
  "name": "Insta Commtest",
  "version": "1.0",
  "description": "Pick out a random comment and check if that user has liked and followed the current account. Try again if it didn't",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "*://*.instagram.com/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "manifest_version": 2
}
